#!/bin/sh

export PATH="@CONFIGURE_PATH@:$PATH"
export LD_LIBRARY_PATH="@CONFIGURE_LD_LIBRARY_PATH@:$LD_LIBRARY_PATH"
export PKG_CONFIG_PATH="@CONFIGURE_PKG_CONFIG_PATH@:$PKG_CONFIG_PATH"
export CPATH="@CONFIGURE_CPATH@:$CPATH"
export LIBRARY_PATH="@CONFIGURE_LIBRARY_PATH@:$LIBRARY_PATH"


if test "x$#" != "x0" -a -z "$MPC_CONFIG_RELOADED"; then
	 "$@"
fi